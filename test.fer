let ar = import('archive/archive');

let writer = ar.new(ar.OPEN_WRITE);
writer.add_filter(ar.FILTER_ZSTD);
writer.set_format(ar.FORMAT_TAR_PAX_RESTRICTED);
writer.open('test.tar.zst');
writer.write_file('README.md');
writer.write_file('LICENSE');
writer.close();

let reader = ar.new(ar.OPEN_READ);
reader.add_filter(ar.FILTER_ZSTD);
reader.set_format(ar.FORMAT_TAR_PAX_RESTRICTED);
reader.open('test.tar.zst');
reader.extract();
reader.close();